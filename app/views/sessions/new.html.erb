<h1>Log in</h1>

  <%= form_with(url: login_path, scope: :session, local: true) do |f| %>

  <%= f.label :email, 'Email' %>
  <%= f.email_field :email %>

  <%= f.label :password, 'Password' %>
  <%= f.password_field :password %>

  <%= f.submit 'Log in' %>
<% end %>

