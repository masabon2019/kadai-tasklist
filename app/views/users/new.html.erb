<h1>SignUp</h1>

<%= form_with(model: @user, local: true) do |f| %>

  <%= f.label :name, 'Name' %>
  <%= f.text_field :name %>
  <br>
  <%= label :email, 'Email' %>
  <%= f.email_field :email %>
  <br>
  <%= label :password, 'Password' %>
  <%= f.password_field :password %>
  <br>
  <%= label :password_confirmation, 'Confirmation' %>
  <%= f.password_field :password_confirmation %>
  <br>
  <%= f.submit 'SignUp' %>
  
<% end %>

